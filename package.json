{
  "name": "ts-nest-bootstrap",
  "private": true,
  "version": "1.0.0",
  "description": "Nest.js proof of concept and base project",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docs": "npx compodoc -p tsconfig.json -s",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migrations-create": "npm run typeorm-cli -- migration:create -d src/migrations/migrations -n",
    "migrations-generate": "npm run typeorm-cli -- migration:generate --config src/config/db.ts -n",
    "migrations-run": "npm run typeorm-cli -- migration:run --config src/config/db.ts",
    "migrations-undo": "npm run typeorm-cli -- migration:revert --config src/config/db.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=testing jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm-cli": "ts-node-dev ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/typeorm": "^6.3.3",
    "@types/bcryptjs": "^2.4.2",
    "apollo-server-express": "^2.11.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "express-wolox-logger": "^2.0.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "jwt-simple": "^0.5.6",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-wolox": "^4.0.0",
    "eslint-config-wolox-node": "^3.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0"
  }
}
